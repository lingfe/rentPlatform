/**index.wxss**/
page{
  background-color: #efeff4;
}
.page{
  width: 100%;
}
.page_myUserInfo{
  width: 100%;
  padding-top: 20rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-color: #330066;
}
.page_myUserInfo_image{
  width: 14%;
  height: 100rpx;
  margin: auto;
}
.page_myUserInfo_image image{
  width: 100%;
  height: 100rpx;
  border-radius:50%;overflow:hidden;display:block;
} 
.page_myUserInfo_userName{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.page_myUserInfo_caozou{
  width: 100%;
  margin: auto;
  height: 120rpx;
  background-color: #fff;
  padding: 10rpx;
}
.page_myUserInfo_caozou_caozou{
  width: 33.3%;
  height: 100rpx;
  float: left;
}
.page_myUserInfo_caozou_caozou_image{
  width: 100%;
  text-align: center;
  color: red;
}
.page_myUserInfo_caozou_caozou_title{
  width: 100%;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
}

/* 菜单  start*/
.page_useCaidang{
  width: 100%;
  margin-top: 30rpx;
  padding-bottom: 30rpx;
  height: auto;
}
.page_caidang{
  width: 100%;
  margin: auto;
  margin-top: 20rpx;
  background-color: #fff;
}
.page_caidang_caidang{
  width: 94%;
  height: 100rpx;
  background-color: #FFf;
  border-bottom: 1rpx #efeff4 solid;
  margin: auto;
}
.page_caidang_caidang_image{
  width: 10%;
  height: 100rpx;
  margin: auto;
  float: left;
}
.page_caidang_caidang_image image{
  width: 90%;
  height: 60rpx;
  margin-top: 15rpx;
}
.page_caidang_caidang_title{
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  margin-left: 5%;
  float: left;

}
.page_caidang_caidang_text{
  width: 35%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
  float: left;
  text-align: right;
}
.border-no{
  border: none;
}
