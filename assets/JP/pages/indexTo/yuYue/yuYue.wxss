 page{
   background-color: #fff;
 }
 .page{
   width: 100%;
   height: 100%;
   padding-top: 20rpx;
 }

 .page_view{
   width: 100%;
   column-count: 2;
   column-gap:0;
 }
 .view_model{
   width: 93.33%;
   background-color: #fff;
   box-shadow: 0rpx 3rpx 5rpx #eee;
   border: 1rpx solid #eee;
   margin: auto;
   margin-bottom: 20rpx;
   break-inside: avoid;/*page-break-inside 属性设置元素内部的 page-breaking 行为。*/
 }
 .view_model_image{
   width: 100%;
   height: 300rpx;
   background-size: 100% 100%;
   background-repeat: no-repeat;
   
 }
  .view_model_title{
    width: 100%;
    margin-top: 10rpx;
    text-align: center;
    font-size: 32rpx;
 }
 .view_model:active{
   background-color: #eee;
   color: #fff;
 }