/* pages/indexTo/admissionApply/admissionApply.wxss */

page{
  background-color:#eee;
}

 .find_img{
   width: 100%;
   height: auto;
 }
 .img{
   width: 100%;
   height: 300rpx;
 }
 .find_menu{
  width: 100%;
  margin-top: 20rpx;
 }
 .menu{
   width: 90%;
   height: 200rpx;
   margin: auto;
   background-color: #fff;
   margin-bottom: 20rpx;
   box-shadow: 2px 2px 5px #888888;
   border-radius: 10rpx;
 }
 .menu:active{
   box-shadow: 2px 2px 5px #aaa;
 }
 .menu_img{
   width: 130rpx;
   height: 130rpx;
   border-radius: 30px;
   margin-left: 20rpx;
   margin-top: 30rpx;
   float: left;
 }
 .menu_text{
   height: 200rpx;
   line-height: 200rpx;
   font-size:40rpx;
   margin-left: 20rpx;
   float: left;
 }
 .menu_v{
  height: 200rpx;
  line-height: 200rpx;
   font-size: 70rpx;
   color: #aaa;
   margin-right: 20rpx;
   float: right;
 }
.menv_v_img{
  position: relative;
  top: 10rpx;
  width: 50rpx;
  height: 50rpx;
}